students = {
  :cohort1 => 34,
  :cohort3 => 22,
  :cohort4 => 43
}
total = 0
students.each do |cohort,number|
  number = number * 1.05
  puts "#{cohort}: #{number.round} students"
  total += number
end

puts students.keys

puts "the total number of students after the %5 increase is #{total.round}"
